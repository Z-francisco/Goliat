{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Goliat": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/Goliat-BETA",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/plugins/fontawesome-free/css/all.min.css",
              "node_modules/plugins/jqvmap/jqvmap.min.css",
              "node_modules/dist/css/adminlte.min.css",
              "node_modules/plugins/overlayScrollbars/css/OverlayScrollbars.min.css",
              "node_modules/plugins/daterangepicker/daterangepicker.css",
              "node_modules/plugins/summernote/summernote-bs4.min.css"
            ],
            "scripts": [
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/plugins/jquery/jquery.min.js",
              "node_modules/plugins/jquery-ui/jquery-ui.min.js",
              "node_modules/plugins/chart.js/Chart.min.js",
              "node_modules/plugins/sparklines/sparkline.js",
              "node_modules/plugins/jqvmap/jquery.vmap.min.js",
              "node_modules/plugins/jqvmap/maps/jquery.vmap.usa.js",
              "node_modules/plugins/jquery-knob/jquery.knob.min.js",
              "node_modules/plugins/moment/moment.min.js",
              "node_modules/plugins/daterangepicker/daterangepicker.js",
              "node_modules/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js",
              "node_modules/plugins/summernote/summernote-bs4.min.js",
              "node_modules/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js",
              "node_modules/dist/js/adminlte.js",
              "node_modules/dist/js/pages/dashboard.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "Goliat:build:production"
            },
            "development": {
              "browserTarget": "Goliat:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "Goliat:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
